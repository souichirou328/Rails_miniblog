<div class="contents row">
  <%= form_tag("/tweets/#{@tweet.id}", method: :patch) do %>
    <h3>
      編集する
    </h3>
    <input type="text" name="image" value="<%= @tweet.image %>" placeholder="Image Url" autofocus="true">
    <input type="text" name="text" placeholder="Single Word">
    <input type="submit" value="SEND">
  <% end %>
</div>